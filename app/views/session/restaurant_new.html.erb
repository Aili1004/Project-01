
<h2>Restaurant User Login</h2>

<%= form_tag '/restaurant_login' do %>
	<p>
		<%= label_tag :email %>
		<%= text_field_tag :email %>
	</p>

	<p>
		<%= label_tag :password %>
		<%= password_field_tag :password %>
	</p>

	<p>
		<%= label_tag :license_number %>
		<%= text_field_tag :license_number %>
	</p>

	<p>
		<%= submit_tag 'Login as Restaurant User' %>
	</p>

<% end %>

<%= link_to "Don't have an account? Click here to sign up! ", new_restaurant_user_path %>